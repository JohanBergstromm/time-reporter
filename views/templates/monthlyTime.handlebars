{{#each response}}
<div class="time-item col-12 mt-2" data-id="{{this._id}}" data-day="{{this.date.day}}">
    <div class="date mb-2 d-none">
        <p>{{this.date.day}}/{{this.date.month}}</p>
    </div>
    <div class="time">
        <p class="m-0 d-flex"><span class="d-inline-block mr-2">Fr√•n: {{this.from.hour}}:{{this.from.minute}}</span> - <span class="d-inline-block ml-2">Till: {{this.to.hour}}:{{this.to.minute}}</span></p>
        <input type="hidden" class="fhDone" name="fromHour" value="{{this.from.hour}}">
        <input type="hidden" class="fmDone" name="fromMin" value="{{this.from.minute}}">
        <input type="hidden" class="thDone" name="toHour" value="{{this.to.hour}}">
        <input type="hidden" class="tmDone" name="toMin" value="{{this.to.minute}}">
        <input type="hidden" class="no-time" value="{{this.noTime}}">
    </div>
    {{#if this.add.comment}}
    <div class="comment">
        <p class="m-0">Kommentar: {{this.add.comment}}</p>
    </div>
    {{/if}}
</div>
{{/each}}